<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  

<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.trans.hourlycommits.model.HourlyCommits"/>
    <class name="org.ossmeter.metricprovider.trans.hourlycommits.model.Hour"/>
  </extension>
<extension
      point="org.ossmeter.platform.metricprovider">
   <metricProvider
         provider="org.ossmeter.metricprovider.trans.hourlycommits.HourlyCommitsMetricProvider">
   </metricProvider>
</extension>
<extension
      point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser
         metricProviderId="org.ossmeter.metricprovider.trans.hourlycommits.HourlyCommitsMetricProvider"
         pongoVisualiser="org.ossmeter.metricprovider.trans.hourlycommits.model.HourlyCommitsViz">
   </visualiser>
</extension>
</plugin>
