package org.ossmeter.metricprovider.generic.newthreadsperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyNewThreads {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
   @searchable
   attr String url_name;
	@yaxis(title="Number of new threads")
   attr int numberOfNewThreads;
}

