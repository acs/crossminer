package org.ossmeter.metricprovider.historic.numberofarticlesperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyNoa {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
   @searchable
   attr String url_name;
	@yaxis(title="Number of articles")
   attr int numberOfArticles;
}

