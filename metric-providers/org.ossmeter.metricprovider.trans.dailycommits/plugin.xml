<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.trans.dailycommits.model.DailyCommits"/>
    <class name="org.ossmeter.metricprovider.trans.dailycommits.model.Day"/>
  </extension>
<extension
      point="org.ossmeter.platform.metricprovider">
   <metricProvider
         provider="org.ossmeter.metricprovider.trans.dailycommits.DailyCommitsMetricProvider">
   </metricProvider>
</extension>
<extension
      point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser
         metricProviderId="org.ossmeter.metricprovider.trans.dailycommits.DailyCommitsMetricProvider"
         pongoVisualiser="org.ossmeter.metricprovider.trans.dailycommits.model.DailyCommitsViz">
   </visualiser>
</extension>
</plugin>
