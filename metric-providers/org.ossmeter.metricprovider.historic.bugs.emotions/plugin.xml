<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  

<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.bugs.emotions.model.BugsEmotionsHistoricMetric"/>
    <class name="org.ossmeter.metricprovider.historic.bugs.emotions.model.BugData"/>
    <class name="org.ossmeter.metricprovider.historic.bugs.emotions.model.Dimension"/>
  </extension>
<extension
      point="org.ossmeter.platform.metricprovider">
   <metricProvider
         provider="org.ossmeter.metricprovider.historic.bugs.emotions.EmotionsHistoricMetricProvider">
   </metricProvider>
</extension>
<extension
      point="org.ossmeter.platform.visualisation.metric">
   <visualisation
         json="vis/historic.bugs.emotions.json"></visualisation>
</extension>
</plugin>
