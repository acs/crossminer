<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  
  <extension point="org.ossmeter.platform.client.api.pongoviz">
     <visualiser metricProviderId="org.ossmeter.metricprovider.generic.numberofusersperday" pongoVisualiser="org.ossmeter.metricprovider.historic.numberofusersperday.model.DailyUsersViz">
     </visualiser>
  </extension>
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.historic.numberofusersperday.NumberOfUsersPerDayProvider">
     </metricProvider>
  </extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.numberofusersperday.model.DailyUsers"/>
    <class name="org.ossmeter.metricprovider.historic.numberofusersperday.model.DailyNewsgroupData"/>
  </extension>
</plugin>
