<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  <extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.generic.numberofusersperday.model.DailyUsers"/>
    <class name="org.ossmeter.metricprovider.generic.numberofusersperday.model.DailyNewsgroupData"/>
  </extension>
  <extension
        point="org.ossmeter.platform.client.api.pongoviz">
     <visualiser
           metricProviderId="org.ossmeter.metricprovider.generic.numberofusersperday"
           pongoVisualiser="org.ossmeter.metricprovider.generic.numberofusersperday.model.DailyUsersViz">
     </visualiser>
  </extension>
  <extension
        point="org.ossmeter.platform.metricprovider">
     <metricProvider
           provider="org.ossmeter.metricprovider.generic.numberofusersperday.NumberOfUsersPerDayProvider">
     </metricProvider>
  </extension>
</plugin>
