package org.ossmeter.metricprovider.bugheadermetadata.model;

@db(qualifiedCollectionNames="true")
@customize
class BugHeaderMetadata {
	val BugData[*] bugs;
}

class BugData {
   @searchable
   attr String url;
   attr String product;
   attr String component;
   attr String bugId;
   attr String status;
   attr String resolution;
   attr String operatingSystem;
   attr String priority;
}

