<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  

<extension point="org.ossmeter.platform.metricprovider">
   <metricProvider provider="org.ossmeter.metricprovider.historic.avgnumberofrequestsreplies.AverageNumberOfRequestsRepliesProvider">
   </metricProvider>
</extension>
<extension point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser metricProviderId="org.ossmeter.metricprovider.generic.avgnumberofrequestsreplies" pongoVisualiser="org.ossmeter.metricprovider.historic.avgnumberofrequestsreplies.model.AverageRRViz">
   </visualiser>
</extension>


<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.avgnumberofrequestsreplies.model.AverageRR"/>
  </extension>
<extension
      point="org.ossmeter.platform.visualisation.metric">
   <visualisation
         json="vis/articlesrequestsreplies.json">
   </visualisation>
</extension>
</plugin>
