<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.4"?><plugin>
   
   <extension point="org.ossmeter.platform.metricprovider">
      <metricProvider provider="org.ossmeter.metricprovider.historic.numberofarticlesperday.NumberOfArticlesPerDayProvider">
      </metricProvider>
   </extension>


<extension point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser metricProviderId="org.ossmeter.metricprovider.generic.numberofarticlesperday" pongoVisualiser="org.ossmeter.metricprovider.historic.numberofarticlesperday.model.DailyNoaViz">
   </visualiser>
</extension>

<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.numberofarticlesperday.model.DailyNoa"/>
    <class name="org.ossmeter.metricprovider.historic.numberofarticlesperday.model.DailyNewsgroupData"/>
  </extension>
</plugin>
