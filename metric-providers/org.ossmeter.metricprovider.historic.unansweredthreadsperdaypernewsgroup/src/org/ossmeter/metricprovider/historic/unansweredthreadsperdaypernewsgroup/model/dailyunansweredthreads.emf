package org.ossmeter.metricprovider.historic.unansweredthreadsperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyUnansweredThreads {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
   @searchable
   attr String url_name;
	@yaxis(title="Number of unanswered threads")
   attr int numberOfUnansweredThreads;
}

