<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  

<extension point="org.ossmeter.platform.metricprovider">
   <metricProvider provider="org.ossmeter.metricprovider.historic.bugs.severity.SeverityHistoricMetricProvider">
   </metricProvider>
</extension>
<extension point="org.ossmeter.platform.visualisation.metric">
   <visualisation json="vis/historic.bugs.severity.json"/>
</extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.bugs.severity.model.BugsSeveritiesHistoricMetric"/>
    <class name="org.ossmeter.metricprovider.historic.bugs.severity.model.BugData"/>
    <class name="org.ossmeter.metricprovider.historic.bugs.severity.model.SeverityLevel"/>
  </extension>
</plugin>
