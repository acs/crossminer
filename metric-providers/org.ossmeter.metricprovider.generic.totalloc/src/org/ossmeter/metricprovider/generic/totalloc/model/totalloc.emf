package org.ossmeter.metricprovider.generic.totalloc.model;

@viz(target="gcharts", chart="line")
class TotalLoc {
	@series(label="url")
	val TLocRepositoryData[*] repositories;
}

class TLocRepositoryData {
   attr String url;
   attr String repoType;
   attr String revision;
   
   @yaxis(title="Total lines of code")
   attr long totalLines;
}
