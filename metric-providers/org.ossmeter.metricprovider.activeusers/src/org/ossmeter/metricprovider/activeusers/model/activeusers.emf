package org.ossmeter.metricprovider.activeusers.model;

@db(qualifiedCollectionNames="true")
@customize
class ActiveUsers {
	val NewsgroupData[*] newsgroups;
    val User[*] users;
}

class NewsgroupData {
	@searchable
   	attr String url_name;
   	attr int numberOfActiveUsers;
}

class User {
   @searchable
   attr String url_name;
   attr String userId;
   attr String lastActivityDate;
}
