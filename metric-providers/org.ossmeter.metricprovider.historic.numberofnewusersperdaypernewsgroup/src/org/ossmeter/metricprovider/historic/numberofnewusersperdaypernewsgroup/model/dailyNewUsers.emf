package org.ossmeter.metricprovider.historic.numberofnewusersperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyNewUsers {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
	@searchable
	attr String url_name;
	@yaxis(title="Number of new users")
	attr int numberOfNewUsers;
}
