<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.generic.numberoffixedbugzillabugs.NumberOfFixedBugzillaBugsProvider">
     </metricProvider>
  </extension>

<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.generic.numberoffixedbugzillabugs.model.DailyNofb"/>
    <class name="org.ossmeter.metricprovider.generic.numberoffixedbugzillabugs.model.DailyBugzillaData"/>
  </extension>
<extension
      point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser
         metricProviderId="org.ossmeter.metricprovider.generic.numberoffixedbugzillabugs"
         pongoVisualiser="org.ossmeter.metricprovider.generic.numberoffixedbugzillabugs.model.DailyNofbViz">
   </visualiser>
</extension>
</plugin>
