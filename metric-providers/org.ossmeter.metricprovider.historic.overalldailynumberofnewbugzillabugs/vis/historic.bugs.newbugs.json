{
	"metricid" : "org.ossmeter.metricprovider.historic.bugs.newbugs",
	"vis" : [ 
		{
			"id" : "bugs.bugs",
			"name" : "Bugs",
			"description" : "The number of bugs submitted every day." ,
			"type" : "LineChart",
			"datatable" : {
				"cols" : [  { "name" : "Date", "field" : "$__date" }, 
					{ "name" : "Bugs", "field" : "$numberOfBugs" } ]
			},
			"x" : "Date",
			"y" : "Bugs",
			"timeSeries" : true
		},{
			"id" : "bugs.cumulativeBugs",
			"name" : "Bugs (Cumulative)",
			"description" : "The number of bugs submitted up to and including the current date." ,
			"type" : "LineChart",
			"datatable" : {
				"cols" : [  { "name" : "Date", "field" : "$__date" }, 
					{ "name" : "Bugs", "field" : "$cumulativeNumberOfBugs" } ]
			},
			"x" : "Date",
			"y" : "Bugs",
			"timeSeries" : true
		}
	]

}
