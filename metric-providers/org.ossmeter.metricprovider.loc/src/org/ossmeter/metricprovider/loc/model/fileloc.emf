package org.ossmeter.metricprovider.loc.model;

@db(qualifiedCollectionNames="true")
@customize
class Loc {
	val RepositoryData[*] repositories;
	val LinesOfCodeData[*] linesOfCode;
}

class RepositoryData {
   @searchable
   attr String url;
   @searchable
   attr String repoType;
   attr String revision;
   ref LinesOfCodeData[*] linesOfCode;
}

class LinesOfCodeData {
   @searchable
   attr String file;
   attr long lines;
   attr String revisionNumber;
   attr String date;
}