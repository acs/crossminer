package org.ossmeter.metricprovider.trans.commits.model;

@db(qualifiedCollectionName="true")
class Commits {
	val RepositoryData[*] repositories;
	val CommitData[*] commits;
}

class RepositoryData {
	@searchable
	attr String url;
	attr String repoType;
	attr String revision;
	ref CommitData[*] commits;
}

class CommitData {
	@searchable
	attr String date;
	attr String time;
}