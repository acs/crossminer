<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperday.NumberOfRequestsRepliesPerDayProvider">
     </metricProvider>
  </extension>

<extension point="org.ossmeter.platform.client.api.pongoviz">
   <visualiser metricProviderId="org.ossmeter.metricprovider.generic.numberofrequestsrepliesperday" pongoVisualiser="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperday.model.DailyNorrViz">
   </visualiser>
</extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.generic.numberofrequestsrepliesperday.model.DailyNorr"/>
    <class name="org.ossmeter.metricprovider.generic.numberofrequestsrepliesperday.model.DailyNewsgroupRequestsData"/>
    <class name="org.ossmeter.metricprovider.generic.numberofrequestsrepliesperday.model.DailyNewsgroupRepliesData"/>
  </extension>
</plugin>
