package org.ossmeter.metricprovider.historic.numberofinactiveusersperday.model;

@viz(target="gcharts", chart="line")
class DailyInactiveUsers {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
	@yaxis(title="Number of inactive users")
	attr int numberOfInactiveUsers;
}
