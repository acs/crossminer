package org.ossmeter.metricprovider.rascal.metric.trans.model;

@db
class Loc {
    val LinesOfCodeData[*] linesOfCode;
}

class LinesOfCodeData {
   @searchable
   attr String file;
   attr long totalLines;
   attr long commentedLines;
   attr long emptyLines;
   attr long sourceLines;
   attr String revisionNumber;
   attr String date;
}