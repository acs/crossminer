<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  

<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.newsgroups.severity.model.NewsgroupsSeveritiesHistoricMetric"/>
    <class name="org.ossmeter.metricprovider.historic.newsgroups.severity.model.NewsgroupData"/>
    <class name="org.ossmeter.metricprovider.historic.newsgroups.severity.model.SeverityLevel"/>
  </extension>
<extension
      point="org.ossmeter.platform.visualisation.metric">
   <visualisation
         json="vis/historic.newsgroups.severity.json">
   </visualisation>
</extension>
<extension
      point="org.ossmeter.platform.metricprovider">
   <metricProvider
         provider="org.ossmeter.metricprovider.historic.newsgroups.severity.SeverityHistoricMetricProvider">
   </metricProvider>
</extension>
</plugin>
