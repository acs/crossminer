     <visualiser metricProviderId="org.ossmeter.metricprovider.generic.numberofnewusersperday" pongoVisualiser="org.ossmeter.metricprovider.historic.numberofnewusersperday.model.DailyNewUsersViz">
     </visualiser>
  </extension>
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.historic.numberofnewusersperday.NumberOfNewUsersPerDayProvider">
     </metricProvider>
  </extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.numberofnewusersperday.model.DailyNewUsers"/>
    <class name="org.ossmeter.metricprovider.historic.numberofnewusersperday.model.DailyNewsgroupData"/>
  </extension>
</plugin>
