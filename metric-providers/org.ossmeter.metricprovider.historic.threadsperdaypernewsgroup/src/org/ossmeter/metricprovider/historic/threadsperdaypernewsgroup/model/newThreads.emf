package org.ossmeter.metricprovider.generic.threadsperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyThreads {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
   @searchable
   attr String url_name;
	@yaxis(title="Number of threads")
   attr int numberOfThreads;
}

