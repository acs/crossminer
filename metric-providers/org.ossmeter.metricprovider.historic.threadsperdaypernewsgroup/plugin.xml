     <visualiser metricProviderId="org.ossmeter.metricprovider.generic.threadsperdaypernewsgroup" pongoVisualiser="org.ossmeter.metricprovider.historic.threadsperdaypernewsgroup.model.DailyThreadsViz">
     </visualiser>
  </extension>
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.historic.threadsperdaypernewsgroup.ThreadsPerDayPerNewsgroupProvider">
     </metricProvider>
  </extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.threadsperdaypernewsgroup.model.DailyThreads"/>
