package org.ossmeter.metricprovider.requestreplyclassification.model;

@db(qualifiedCollectionNames="true")
@customize
class Rrc {
	val BugzillaCommentsData[*] bugzillaComments;
	val NewsgroupArticlesData[*] newsgroupArticles;
}

class BugzillaCommentsData {
	@searchable
   	attr String url;
	attr String prod;
   	attr String comp;
   	attr String bugId;
   	attr String commentId;
   	attr String classificationResult;
}

class NewsgroupArticlesData {
	@searchable
   	attr String url;
   	attr int articleNumber;
   	attr String classificationResult;
}
