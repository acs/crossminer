package org.ossmeter.metricprovider.generic.numberofusersperdaypernewsgroup.model;

@viz(target="gcharts", chart="line")
class DailyUsers {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
	@searchable
	attr String url_name;
	@yaxis(title="Number of users")
	attr int numberOfUsers;
}
