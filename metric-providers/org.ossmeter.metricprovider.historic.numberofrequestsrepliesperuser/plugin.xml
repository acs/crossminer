     <visualiser metricProviderId="org.ossmeter.metricprovider.generic.numberofrequestsrepliesperuser" pongoVisualiser="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperuser.model.AverageRRUserViz">
     </visualiser>
  </extension>
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperuser.NumberOfRequestsRepliesPerUserProvider">
     </metricProvider>
  </extension>
<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperuser.model.AverageRRUser"/>
    <class name="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperuser.model.AverageArticlesPerUserData"/>
    <class name="org.ossmeter.metricprovider.historic.numberofrequestsrepliesperuser.model.AverageRequestsPerUserData"/>
