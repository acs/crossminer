<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
  
  <extension point="org.ossmeter.platform.metricprovider">
     <metricProvider provider="org.ossmeter.metricprovider.trans.topics.TopicsTransMetricProvider">
     </metricProvider>
  </extension>
  <extension point="org.ossmeter.platform.visualisation.metric">
     <visualisation json="vis/trans.topics.json">
     </visualisation>
  </extension>




<extension point="com.googlecode.pongo.runtime.osgi">
    <class name="org.ossmeter.metricprovider.trans.topics.model.TopicsTransMetric"/>
    <class name="org.ossmeter.metricprovider.trans.topics.model.BugTrackerCommentsData"/>
    <class name="org.ossmeter.metricprovider.trans.topics.model.NewsgroupArticlesData"/>
    <class name="org.ossmeter.metricprovider.trans.topics.model.BugTrackerTopic"/>
    <class name="org.ossmeter.metricprovider.trans.topics.model.NewsgroupTopic"/>
  </extension>
</plugin>
