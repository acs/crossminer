package org.ossmeter.metricprovider.historic.overallsentimentnntp.model;

@viz(target="gcharts", chart="line")
class OverallSentimentNNTP {
	@series
	val DailyNewsgroupData[*] newsgroups;
}

class DailyNewsgroupData {
	@yaxis(title="Overall Sentiment of Bugs")
	attr float overallAverageSentiment;
	attr float overallSentimentAtThreadBeggining;
	attr float overallSentimentAtThreadEnd;
}

