  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VcsRepository" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="created_at" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
