{
	"metricid" : "org.ossmeter.metricprovider.historic.commitsovertime",
	"vis" : [ 
		{
			"name" : "commitsovertimeline",
			"type" : "LineChart",
			"datatable" : {
				"rows" : "$repositories",
				"cols" : [  { "name" : "Date", 			"field" : "$__date" }, 
							{ "name" : "Commits", 		"field" : "$numberOfCommits" },
							{ "name" : "Repository", 	"field" : "$url" } ]
			},
			"x" : "Date",
			"y" : "Commits", 
			"series" : "Repository",
			"x-label" : "Date",		
			"y-label" : "Commits"	
		}
	]
}